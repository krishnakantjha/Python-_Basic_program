    "def fact(num):\n",
    "    if num==1 or num == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return num*fact(num-1)\n",
    "num= int(input())\n",
    "if num<0:\n",
    "    print('Please enter positive no.')\n",
    "    \n",
    "else:\n",
    "    print(fact(num))\n",
